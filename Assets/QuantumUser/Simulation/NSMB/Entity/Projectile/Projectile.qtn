import enum ParticleEffect(byte);

component Projectile {
	asset_ref<ProjectileAsset> Asset;
	FP Speed;
	EntityRef Owner;
	bool FacingRight;
	bool HasBounced;
	bool PlayDestroySound;
	bool CheckedCollision;
}

event ProjectileDestroyed {
	nothashed Frame Frame;
	EntityRef Entity;
	ParticleEffect Particle;
	nothashed FPVector2 Position;
}